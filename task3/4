#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n, a1;
    scanf("%d", &n);
    a1 = 0;
    while (a1 > 9 || a1 == 0)
    {
       a1 = 0;
       while (n != 0)
        {

        a1 = a1 + n%10;
        n = n / 10;

        }
        n = a1;

    }

    printf("Digital root: %8d", a1);


    return 0;
}
